{
  "name": "my-app",
  "description": "Awsome Application",
  "author": "Maximilian Pfitzenmaier",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx prettier --write . && vite --host --open",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
